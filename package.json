{
    "name": "integrate-dropbox-wp",
    "version": "1.0.0",
    "description": "Integrate Dropbox is a WordPress plugin that allows you to integrate your Dropbox account with your WordPress website. You can upload files to your Dropbox account from your WordPress website and also you can download files from your Dropbox account to your WordPress website.",
    "main": "src/index.js",
    "scripts": {
        "start": "wp-scripts start --config .dev/webpack.config.js --progress",
        "build": "wp-scripts build --config .dev/webpack.config.js",
        "start:file-browser": "wp-scripts start src/file-browser/index.js --output-path=assets/admin/file-browser/",
        "build:file-browser": "wp-scripts build src/file-browser/index.js --output-path=assets/admin/file-browser/",
        "start:settings": "wp-scripts start src/settings/index.js --output-path=assets/admin/settings/",
        "build:settings": "wp-scripts build src/settings/index.js --output-path=assets/admin/settings/",
        "start:shortcode-builder": "wp-scripts start src/shortcode-builder/index.js --output-path=assets/admin/shortcode-builder/",
        "build:shortcode-builder": "wp-scripts build src/shortcode-builder/index.js --output-path=assets/admin/shortcode-builder/",
        "start:admin:common": "wp-scripts start src/common/index.js --output-path=assets/admin/common/",
        "build:admin:common": "wp-scripts build src/common/index.js --output-path=assets/admin/common/",
        "start:all": "npm-run-all --parallel start:file-browser start:settings start:shortcode-builder start:admin:common",
        "build:all": "npm-run-all --parallel build:file-browser build:settings build:shortcode-builder build:admin:common"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ultraDevs/integrate-dropbox-wp"
    },
    "keywords": [
        "plugin"
    ],
    "author": "ultraDevs",
    "license": "GPL",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@wordpress/scripts": "^27.6.0",
        "autoprefixer": "^10.4.13",
        "css-loader": "^6.7.3",
        "grunt": "^1.6.1",
        "grunt-autoprefixer": "^3.0.4",
        "grunt-checktextdomain": "^1.0.1",
        "grunt-contrib-clean": "^2.0.1",
        "grunt-contrib-compress": "^2.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-jshint": "^3.2.0",
        "grunt-contrib-sass": "^2.0.0",
        "grunt-contrib-uglify": "^5.2.2",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-wp-readme-to-markdown": "^2.1.0",
        "mini-css-extract-plugin": "^2.7.2",
        "node-sass": "^9.0.0",
        "node-sass-glob-importer": "^5.3.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "postcss-color-function": "^4.1.0",
        "react-transition-group": "^4.4.5",
        "rtlcss-webpack-plugin": "^4.0.7",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "webpack-fix-style-only-entries": "^0.6.1"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.11",
        "@wordpress/dom-ready": "^3.27.0",
        "@wordpress/icons": "^9.18.0",
        "classnames": "^2.3.2",
        "grunt-contrib-rename": "^0.2.0",
        "js-base64": "^3.7.7",
        "lightgallery": "^2.7.2",
        "react": "^18.2.0",
        "react-contexify": "^6.0.0",
        "react-dom": "^18.2.0",
        "sass": "^1.58.3",
        "tailwindcss": "^3.2.7"
    }
}
