{
    "name": "integrate-dropbox",
    "version": "1.0.0",
    "description": "Integrate Dropbox is a WordPress plugin that allows you to integrate your Dropbox account with your WordPress website. You can upload files to your Dropbox account from your WordPress website and also you can download files from your Dropbox account to your WordPress website.",
    "main": "src/index.js",
    "scripts": {
        "start": "wp-scripts start --config .dev/webpack.config.js --progress",
        "build": "wp-scripts build --config .dev/webpack.config.js",
        "start:admin": "wp-scripts start src/settings/index.js --output-path=assets/admin/",
        "build:admin": "wp-scripts build src/settings/index.js --output-path=assets/admin/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ultraDevs/integrate-dropbox"
    },
    "keywords": [
        "plugin"
    ],
    "author": "ultraDevs",
    "license": "GPL",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@wordpress/scripts": "^18.1.0",
        "autoprefixer": "^10.3.7",
        "css-loader": "^6.4.0",
        "grunt": "^1.3.0",
        "grunt-autoprefixer": "^3.0.4",
        "grunt-checktextdomain": "^1.0.1",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-contrib-compress": "^2.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-jshint": "^3.0.0",
        "grunt-contrib-sass": "^2.0.0",
        "grunt-contrib-uglify": "^5.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-text-replace": "^0.4.0",
        "grunt-wp-i18n": "^1.0.3",
        "grunt-wp-readme-to-markdown": "^2.0.1",
        "mini-css-extract-plugin": "^2.4.2",
        "node-sass": "^6.0.1",
        "node-sass-glob-importer": "^5.3.2",
        "postcss": "8.3.9",
        "postcss-color-function": "^4.1.0",
        "postcss-loader": "^6.2.0",
        "react-transition-group": "^4.4.1",
        "rtlcss-webpack-plugin": "^4.0.3",
        "sass-loader": "^12.2.0",
        "style-loader": "^3.3.0",
        "webpack-fix-style-only-entries": "^0.6.1"
    },
    "dependencies": {
        "@wordpress/dom-ready": "^3.2.2",
        "@wordpress/icons": "^6.0.1",
        "grunt-contrib-rename": "^0.2.0",
        "react": "^17.0.2",
        "react-toastify": "^8.0.3",
        "tailwindcss": "^2.2.17"
    }
}
